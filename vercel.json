{
    "buildCommand": "npm run build",
    "outputDirectory": "dist/public",
    "functions": {
        "api/index.ts": {
            "runtime": "vercel-node@4"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}