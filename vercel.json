{
    "version": 2,
    "buildCommand": "cd client && npm install && npm run build && cd ..",
    "outputDirectory": "public",
    "functions": {
        "api/index.ts": {
            "runtime": "vercel-node@4.0.0"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}